{
  "multipart": [
    {
      "when": { "facing": "down" },
      "apply": { "model": "lacrimis:block/conduit/one_way_node", "x": 180}
    },
    {
      "when": { "down": true },
      "apply": { "model": "lacrimis:block/conduit/side" }
    },
    {
      "when": { "facing": "up" },
      "apply": { "model": "lacrimis:block/conduit/one_way_node" }
    },
    {
      "when": { "up": true },
      "apply": { "model": "lacrimis:block/conduit/side", "x": 180 }
    },
    {
      "when": { "facing": "north" },
      "apply": { "model": "lacrimis:block/conduit/one_way_node", "x": 90 }
    },
    {
      "when": { "north": true },
      "apply": { "model": "lacrimis:block/conduit/side", "x": 90, "y": 180 }
    },
    {
      "when": { "facing": "south" },
      "apply": { "model": "lacrimis:block/conduit/one_way_node", "x": -90 }
    },
    {
      "when": { "south": true },
      "apply": { "model": "lacrimis:block/conduit/side", "x": 90 }
    },
    {
      "when": { "facing": "west" },
      "apply": { "model": "lacrimis:block/conduit/one_way_node", "x": 90, "y": -90 }
    },
    {
      "when": { "west": true },
      "apply": { "model": "lacrimis:block/conduit/side", "x": 90, "y": 90 }
    },
    {
      "when": { "facing": "east" },
      "apply": { "model": "lacrimis:block/conduit/one_way_node", "x": 90, "y": 90 }
    },
    {
      "when": { "east": true },
      "apply": { "model": "lacrimis:block/conduit/side", "x": 90, "y": 270 }
    }
  ]
}